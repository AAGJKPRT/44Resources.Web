@{
    ViewBag.Title = "Sign Up";
    Layout = null;
}
@model _44Resources.DataContract.BusinessAssociate

<!doctype html>
<html>
<!-- Mirrored from lab.westilian.com/matmix/list-view/index.html by HTTrack Website Copier/3.x [XR&CO'2014], Sun, 08 May 2016 14:54:55 GMT -->
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="A Components Mix Bootstarp 3 Admin Dashboard Template">
    <meta name="author" content="Westilian">

    <link rel="stylesheet" href="~/Content/theme/css/font-awesome.css" type="text/css">
    <link rel="stylesheet" href="~/Content/theme/css/bootstrap.css" type="text/css">
    <link rel="stylesheet" href="~/Content/theme/css/animate.css" type="text/css">
    <link rel="stylesheet" href="~/Content/theme/css/waves.css" type="text/css">
    <link rel="stylesheet" href="~/Content/theme/css/layout.css" type="text/css">
    <link rel="stylesheet" href="~/Content/theme/css/components.css" type="text/css">
    <link rel="stylesheet" href="~/Content/theme/css/plugins.css" type="text/css">
    <link rel="stylesheet" href="~/Content/theme/css/common-styles.css" type="text/css">
    <link rel="stylesheet" href="~/Content/theme/css/pages.css" type="text/css">
    <link rel="stylesheet" href="~/Content/theme/css/responsive.css" type="text/css">
    <link rel="stylesheet" href="~/Content/theme/css/matmix-iconfont.css" type="text/css">
    <link href="http://fonts.googleapis.com/css?family=Roboto:400,300,400italic,500,500italic" rel="stylesheet" type="text/css">
    <link href="http://fonts.googleapis.com/css?family=Open+Sans:400italic,600italic,400,600" rel="stylesheet" type="text/css">
</head>
<body>
    <div class="page-container list-menu-view">
        <div>
            <div class="main-container">
                <div class="container-fluid">
                    <div class="page-breadcrumb">
                        <div class="row">
                            <div class="col-md-7">
                                <div class="page-breadcrumb-wrap">
                                    <div class="page-breadcrumb-info">
                                        <h2 class="breadcrumb-titles">Sign Up for 44 Resources<small></small></h2>
                                        <ul class="list-page-breadcrumb">
                                            <li>
                                                <a href="#">Home</a>
                                            </li>
                                            <li class="active-page"> Sign Up</li>
                                        </ul>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-5">
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-lg-2">
                        </div>
                        <div class="col-lg-8">
                            <div class="box-widget widget-module">
                                <div class="widget-head clearfix">
                                    <span class="h-icon"><i class="fa fa-bars"></i></span>
                                    <h4>44 Resources - Business Associate</h4>
                                </div>
                                <div class="widget-container">
                                    <div class="widget-block">
                                        @*<form id="AjaxSubmit" method="post" class="form-horizontal" action="/home/index">*@
                                        @using (Html.BeginForm("SignUp", "Home", FormMethod.Post, new { @class = "form-horizontal" }))
                                        {
                                            <form>
                                                <div class="form-group">
                                                    <label class="col-lg-4 control-label">FirstName</label>
                                                    <div class="col-lg-8">
                                                        @Html.TextBoxFor(model => model.FirstName, null, new { @class = "form-control" })
                                                        @*<input type="text" required class="form-control" name="username" />*@
                                                    </div>
                                                </div>
                                                <div class="form-group">
                                                    <label class="col-lg-4 control-label">LastName</label>
                                                    <div class="col-lg-8">
                                                        @Html.TextBoxFor(model => model.LastName, null, new { @class = "form-control" })
                                                        @*<input type="text" required class="form-control" name="username" />*@
                                                    </div>
                                                </div>
                                                <div class="form-group">
                                                    <label class="col-lg-4 control-label">Email address</label>
                                                    <div class="col-lg-8">
                                                        @Html.TextBoxFor(model => model.Email, null, new { @class = "form-control" })
                                                        @*<input type="email" required class="form-control" name="email" />*@
                                                    </div>
                                                </div>
                                                <div class="form-group">
                                                    <label class="col-lg-4 control-label">Mobile</label>
                                                    <div class="col-lg-8">
                                                        @Html.TextBoxFor(model => model.Mobile, null, new { @class = "form-control" })
                                                        @*<input type="text" required class="form-control" name="Mobile" />*@
                                                    </div>
                                                </div>
                                                <div class="form-group">
                                                    <label class="col-lg-4 control-label">Password</label>
                                                    <div class="col-lg-8">
                                                        @Html.TextBoxFor(model => model.Password, null, new { @class = "form-control" })
                                                        @*<input type="password" required class="form-control" name="password" />*@
                                                    </div>
                                                </div>
                                                <div class="form-group">
                                                    <label class="col-lg-4 control-label">Confirm Password</label>
                                                    <div class="col-lg-8">
                                                        @Html.TextBoxFor(model => model.ConfirmPassword, null, new { @class = "form-control" })
                                                        @*<input type="password" required class="form-control" name="ConfirmPassword" />*@
                                                    </div>
                                                </div>
                                                <div class="col-lg-8 col-lg-offset-4">
                                                    <div class="checkbox">
                                                        <input type="checkbox" name="acceptTerms" data-bv-field="acceptTerms">
                                                        I agree with terms and conditions
                                                    </div>
                                                </div>
                                                <div class="form-group">
                                                    <div class="col-lg-8 col-lg-offset-4">
                                                        @*@Html.ActionLink("SignUp", "Signup", "Home", new { Model })*@
                                                        <button type="submit" class="btn btn-primary">Sign up!</button>
                                                    </div>
                                                </div>
                                            </form>
                                        }
                                        @*</form>*@
                                    </div>
                                </div>
                            </div>

                        </div>
                        <div class="col-lg-2">
                        </div>
                    </div>
                </div>
            </div>
            <!--Footer Start Here -->
            <footer class="footer-container">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-md-6 col-sm-6">
                            <div class="footer-left">
                                <span>&copy; 2016 <a href="http://www.44resources.com/">44 Resources</a></span>
                            </div>
                        </div>
                        <div class="col-md-6 col-sm-6">
                            <div class="footer-right">
                                <span class="footer-meta">Crafted with&nbsp;<i class="fa fa-heart"></i>&nbsp;by&nbsp;<a href="#">44 Resources</a></span>
                            </div>
                        </div>
                    </div>
                </div>
            </footer>
        </div>
    </div>
    <script src="~/Content/theme/js/jquery-1.11.2.min.js"></script>
    <script src="~/Content/theme/js/jquery-migrate-1.2.1.min.js"></script>
    <script src="~/Content/theme/js/jRespond.min.js"></script>
    <script src="~/Content/theme/js/bootstrap.min.js"></script>
    <script src="~/Content/theme/js/nav-accordion.js"></script>
    <script src="~/Content/theme/js/hoverintent.js"></script>
    <script src="~/Content/theme/js/waves.js"></script>
    <script src="~/Content/theme/js/switchery.js"></script>
    <script src="~/Content/theme/js/jquery.loadmask.js"></script>
    <script src="~/Content/theme/js/icheck.js"></script>
    <script src="~/Content/theme/js/select2.js"></script>
    <script src="~/Content/theme/js/bootstrap-filestyle.js"></script>
    <script src="~/Content/theme/js/bootbox.js"></script>
    <script src="~/Content/theme/js/animation.js"></script>
    <script src="~/Content/theme/js/colorpicker.js"></script>
    <script src="~/Content/theme/js/bootstrap-datepicker.js"></script>
    <script src="~/Content/theme/js/sweetalert.js"></script>
    <script src="~/Content/theme/js/moment.js"></script>
    <script src="~/Content/theme/js/calendar/fullcalendar.js"></script>
</body>
